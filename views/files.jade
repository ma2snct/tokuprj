extends layout

block content
  div.container
    h2 HL7フォーマット

    form(action="/parsehl7", enctype="multipart/form-data", method="POST")
      input(type="file", name="avatar", id="avatar")
      input.btn.btn-primary(type="submit", name="submit", value="submit")

    h2 縦向きCSV(医療大検査データ入力用)

    form(action="/parse", enctype="multipart/form-data", method="POST")
      input(type="file", name="avatar", id="avatar")
      input.btn.btn-primary(type="submit", name="submit", value="submit")

    h2 横向きCSV(医療大投薬データ入力用)

    form(action="/horizontialparse", enctype="multipart/form-data", method="POST")
      input(type="file", name="avatar", id="avatar")
      input.btn.btn-primary(type="submit", name="submit", value="submit")
